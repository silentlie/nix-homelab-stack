# Infrastructure host environment variables
# Copy to .env and fill in actual values

# DNS
DNS_PORT=53
TECHNITIUM_UI_PORT=5380

# Traefik
TRAEFIK_HTTP_PORT=80
TRAEFIK_HTTPS_PORT=443
TRAEFIK_DASHBOARD_PORT=8080
LETSENCRYPT_EMAIL=admin@example.com

# Nginx
NGINX_PORT=8081

# Vault
VAULT_PORT=8200

# Authentik
AUTHENTIK_DB_USER=authentik
AUTHENTIK_DB_PASSWORD=CHANGE_ME_STRONG_PASSWORD
AUTHENTIK_SECRET_KEY=CHANGE_ME_RANDOM_KEY
AUTHENTIK_PORT=9000
AUTHENTIK_HTTPS_PORT=9443

# Vaultwarden
VAULTWARDEN_DB_URL=postgresql://vaultwarden:CHANGE_ME_PASSWORD@postgres/vaultwarden
VAULTWARDEN_DOMAIN=https://vaultwarden.example.com
VAULTWARDEN_PORT=8000
